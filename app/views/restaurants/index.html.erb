<h1>Index</h1>

<div class="restaurants">
  <ul class="restaurants">
    <% @restaurants.each do |r| %>
      <li class="restaurant" data-lat=<%=r.longitude %> data-lon=<%= r.latitude %> %>
        <%= r.name %>
        <%= r.address %>
      </li>
    <% end %>
  </ul>
</div>

<div id="map"></div>
